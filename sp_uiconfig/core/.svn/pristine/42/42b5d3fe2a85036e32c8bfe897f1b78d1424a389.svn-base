@charset "utf-8";


@import "common/splitter.scss";
@import "common/button.scss";
@import "common/window.scss";
@import "common/combobox.scss";
@import "common/gridlayout.scss";
@import "common/textfield.scss";
@import "common/table.scss";
@import "common/dialog.scss";

@mixin uiconfig-sp{
	$height:26px;
	$color: #5a6066;
	$prompt-text-color: #CACACA;
	$font-size: 9px;
	$font-style:normal;
	$font-family: Arial, Verdana, Tahoma, "Microsoft YaHei","微软雅黑",'Open Sans';
	$font: $font-style $font-size $font-family;
	
	
	@include uiconfig-splitter;
	@include dashboard-button;
	@include uiconfig-window;
	@include uiconfig-combobox;
	@include dashboard-gridlayout;
	@include uiconfig-textfield;
	@include uiconfig-table;
	@include uiconfig-dialog;
	
	.main-view{
		position: relative;
		z-index: 2;
		@include fade-in;
		background: #f1f1f1;
	}
	
	.v-animate-reveal {
		@include reveal;
	}
	
	.v-animate-hide {
		@include hide;
	}
	
	
	.sidebar {
		background:#1c69b0;
		color:#fff;		
		@include move-right;
		-webkit-user-select: none;
		-moz-user-select: none;
		-ms-user-select: none;
		user-select: none;
		-webkit-box-shadow: inset -1px 0 0 rgba(255,255,255,.05);
		-moz-box-shadow: inset -1px 0 0 rgba(255,255,255,.05);
		box-shadow: inset -1px 0 0 rgba(255,255,255,.05);
	}
	
	.sidebar .branding {
		position: relative;
		-webkit-font-smoothing: antialiased;
		height:70px;
		background: url('img/sps-logo_yellowblue.png') no-repeat top left;
		
	}
	
