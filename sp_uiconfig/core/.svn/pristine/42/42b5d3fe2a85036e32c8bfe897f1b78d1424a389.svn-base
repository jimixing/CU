	.sidebar .menu {
		overflow-y: auto;
		height: 100%;
		min-width: 200px;
	}
		
	.sidebar .menu .nav-button-setting {
		-webkit-appearance: none;
		display: block;
		padding-top: 11px;
		padding-bottom: 12px;
		padding-left:11px;
		padding-right:10px;
		width: 100%;
		margin: 0;
		position: relative;
		color: #fff;
		font-weight: normal;
		font-size: 11px;
		text-align: left !important;
		border-bottom:1px solid #1663aa;
		border-top: 1px solid rgba(255,255,255,0.05);
		background: #1c69b0;
		text-shadow:0px 1px 0px #073763;
		transition:All 0.2s ease;
		-webkit-transition:All 0.2s ease;
		-moz-transition:All 0.2s ease;
		cursor: pointer;
		-webkit-box-shadow: inset -1px 0 0 rgba(255,255,255,0.05);
		-moz-box-shadow: inset -1px 0 0 rgba(255,255,255,0.05);
		box-shadow: inset -1px 0 0 rgba(255,255,255,0.05);

		>div:nth-child(2)>div{
			display:inline-block;
			margin:0;
			padding:0;
			text-align: left;
			width: auto;
			font-family: 'Open Sans' !important;
			font-size: 11px;
			cursor:pointer;
			}
	}
	
	.sidebar .menu .nav-button-setting:last-child{
	}
		
	.sidebar .menu:after {
		content: "";
		display: block;
		border-top: 1px solid rgba(255,255,255,.05);
		height: 0;
	}
	
	.sidebar .menu .nav-button-setting:focus {
		outline: none;
	}
	
	.sidebar .menu .nav-button-setting:hover,
	.sidebar .menu .nav-button-setting:focus {
		background: #508fcc;
		text-shadow:0px 1px 0px #073763;
		color: #fff;
	}
	
	.sidebar .menu .nav-button-setting:active {
		background: #508fcc;
		text-shadow:0px 1px 0px #073763;
		color: #f5f5f5;
	}
	.sidebar .menu .nav-button-setting.selected {
		background: #508fcc;
		text-shadow:0px 1px 0px #073763;
		color: #fff;		
	}
	.sidebar .menu .nav-button-setting.selected::before{
		content: "\f0d9";
		font-family:'FontAwesome';
		display: block;
		height: 27px;
		width: 27px;
		position: absolute;
		right: -21px;
		line-height: 27px;
		top:50%;
		margin-top:-13px;
		font-size: 20px;
		color: #f1f1f1;
		text-shadow:none !important;
		transition:All 0.2s ease;
		-webkit-transition:All 0.2s ease;
		-moz-transition:All 0.2s ease;
	}
	
	.sidebar .user {
		background-color:#508fcc;
		display: block;
		box-shadow:inset 0 1px 0 rgba(255,255,255,0.05);
		height:90px;
		width: 100%;
		color: #fff;
		text-shadow:0px 1px 0px #073763;
	}
	.sidebar .user .user_head{
		height: 40px;
		width:auto;
		padding-top: 7px;
		padding-bottom: 7px;
		padding-left: 11px;
		margin-right: 10px;
		color: #fff;
	}
	.sidebar .user .user_head::before{
		font-family:'iconfont_sp20';
		content: "\e617";		
		font-size: 22px;
		color:#015a9e;
		display: inline-block;
		height: 26px;
		width: 26px;
		padding-top: 4px;
		box-sizing: border-box;
		overflow: hidden;
		border-radius: 13px;
		text-align: center;
		border:1px solid #fff;
		background: #fff;
		text-shadow:none;
	}
	.sidebar .user .user_name{
		line-height: 40px;
		vertical-align: middle;
	}
	.sidebar .user .user_control_wrapper{
		width: 100%;
		height: 50px;
		color: #fff;
		border-top: 1px solid #629ad1;
	}
	.sidebar .user .user_control_wrapper .v-slot{
		width: 50%;
		height: 50px;
		display: inline-block;
	}
	.sidebar .user .user_settings{
		width:100%;
		height: 50px;
		display: inline-block;
		border-right: 1px solid #5d656f;
		background: #353e4b;
		color:#fff;
		text-shadow:0px 1px 0px #000;
	}
	.sidebar .user .user_settings .v-menubar-menuitem-user_settings_item{
		display: block;
		width: 100%;
		height: 100%;
		cursor:pointer;
		text-align: center;
		line-height: 50px;
		transition:All 0.2s ease;
		-webkit-transition:All 0.2s ease;
		-moz-transition:All 0.2s ease;
	}
	.v-menubar-popup {
	}
	.sidebar .user .user_settings .v-menubar-menuitem-user_settings_item:hover{
		background: #495362;
	}
	.sidebar .user .user_settings .v-menubar-menuitem-caption:after{
		font-family:'FontAwesome'!important;
    	font-size:12px;
    	margin-left: 3px;
    	vertical-align: bottom;
		content: "\f013";
	}
	.sidebar .user .logout{
		width:50%;
		height: 50px;
		display: inline-block;
		background: #353e4b;
		color:#fff;
		border: none;
		text-shadow:0px 1px 0px #000;
		transition:All 0.2s ease;
		-webkit-transition:All 0.2s ease;
		-moz-transition:All 0.2s ease;
		outline: none;
	}
	.sidebar .user .v-nativebutton{
		width: 100%;
		height: 100%;
		border: none;
		padding:0;
	}
	.sidebar .user .v-nativebutton-caption{
		display: block;
		width: 100%;
		height: 100%;
		line-height: 50px;
/*		vertical-align: top !important;*/
	}
	.sidebar .user .v-nativebutton:hover{
		background: #495362;
	}
	.sidebar .user .v-nativebutton-caption:after{
		font-family:'FontAwesome'!important;
    	font-size:12px;
    	margin-left: 3px;
    	vertical-align: bottom !important;
		content: "\f08b";      /* Log out */
	}
	
